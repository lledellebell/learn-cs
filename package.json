{
  "name": "learn-cs",
  "version": "1.0.0",
  "description": "Computer Science learning materials with private content management",
  "scripts": {
    "docs:update": "node scripts/generate-navigation.js && node scripts/update-readme.js",
    "nav:generate": "node scripts/generate-navigation.js",
    "nav:update": "node scripts/update-readme.js",
    "search": "node scripts/search.js",
    "stats": "node scripts/stats.js",
    "balance": "node scripts/learning-balance.js",
    "learning:check": "node scripts/learning-balance.js",
    "docs:hook": "node scripts/auto-commit-hook.js",
    "docs:full-update": "npm run docs:update",
    "feed:generate": "node scripts/generate-feeds.js",
    "feed:rss": "node scripts/generate-rss.js",
    "feed:json": "node scripts/generate-json-feed.js",
    "dates:update": "node scripts/update-dates.js",
    "news:fetch": "node scripts/fetch-hackernews.js",
    "news:fetch-translated": "ENABLE_TRANSLATION=true node scripts/fetch-hackernews.js",
    "publish": "npm run dates:update && npm run docs:update && npm run feed:generate",
    "setup:submodule": "bash scripts/setup-submodule.sh",
    "private:status": "node scripts/manage-private-content.js status",
    "private:update": "node scripts/manage-private-content.js update",
    "private:commit": "node scripts/manage-private-content.js commit",
    "graph:generate": "node scripts/generate-knowledge-graph.js",
    "graph:update": "npm run graph:generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lledellebell/learn-cs.git"
  },
  "keywords": [
    "computer-science",
    "algorithms",
    "data-structures",
    "learning",
    "documentation"
  ],
  "author": "lledellebell",
  "license": "MIT"
}
